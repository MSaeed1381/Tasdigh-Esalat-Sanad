                      textcolor="#474747" textsize="16"/>
                <imagebutton width="24" height="24" src="images/chevron-down.png"/>
            </frame>

            <frame margin_top="12" layout="linear_horizontal" height="wrap_content" weightsum="2" reverse="true">

                <frame margin_top="12" width="wrap_content" weight="1" childgravity="right" layout="linear_vertical">
