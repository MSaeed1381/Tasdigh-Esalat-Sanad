                          textcolor="#2E2E2E" textsize="14" text="حسن" />

                    <text height="wrap_content" width="wrap_content" gravity="right" textalignment="right"
                          textcolor="#616161" textsize="14" text="شماره شناسنامه:" margin_top="16"/>
                    <text height="wrap_content" width="wrap_content" gravity="right" textalignment="right"
                          textcolor="#2E2E2E" textsize="14" text="۱۵۰۱۱۴۲۴۲" />

                </frame>

                <frame margin_top="12" width="wrap_content" weight="1" layout="linear_vertical" height="wrap_content" gravity="top_right">
                    <text height="wrap_content" width="wrap_content" gravity="right" textalignment="right"
                          textcolor="#616161" textsize="14" text="نام خانوادگی:"/>
