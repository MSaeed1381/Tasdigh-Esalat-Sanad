<page id="result" scrollable="true" height="match_parent">
    <frame layout="frame" width="match_parent" gravity="right">
        <imagebutton width="24" height="24" selfgravity="right" background="#4ca9bf" src="images/x.png"
        onclick="app/back"/>
    </frame>
    <text text="استعلام" gravity="right" textsize="20" textcolor="#2E2E2E"/>

